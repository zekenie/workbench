    "@tldraw/sync-core": "^3.4.1",
    "@types/bun": "^1.1.13",
    "@types/lodash-es": "^4.17.12",
    "elysia": "^1.1.25",
    "jose": "^5.9.6",
    "lodash-es": "^4.17.21",
